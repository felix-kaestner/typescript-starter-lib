{
  "name": "typeScript-starter-lib",
  "version": "0.0.0",
  "description": "A basic batteries included template for building TypeScript libraries",
  "main": "dist/index.cjs.js",
  "module": "dist/index.esm.js",
  "browser": "dist/index.umd.js",
  "umd:main": "dist/index.umd.js",
  "unpkg": "dist/index.global.js",
  "jsdelivr": "dist/index.global.js",
  "jsnext:main": "dist/index.esm.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "exports": {
    ".": {
      "import": "./dist/index.esm.js",
      "default": "./dist/index.esm.js",
      "require": "./dist/index.cjs.js",
      "script": "./dist/index.umd.js"
    }
  },
  "repository": "git://github.com/felix-kaestner/typescript-starter-lib",
  "homepage": "https://github.com/felix-kaestner/typescript-starter-lib",
  "license": "MIT",
  "sideEffects": false,
  "author": {
    "name": "Felix KÃ¤stner",
    "email": "hello@felix-kaestner.com",
    "url": "https://felix-kaestner.com"
  },
  "scripts": {
    "dev": "rollup -c -w",
    "build": "rollup -c",
    "build:types": "tsc --declaration --emitDeclarationOnly --declarationDir types",
    "test": "jest",
    "test:dev": "jest --watchAll",
    "test:coverage": "jest --coverage",
    "lint": "eslint .",
    "lint:fix": "eslint --fix .",
    "lint:types": "tsc --pretty --noEmit",
    "format": "prettier --check .",
    "format:fix": "prettier --write .",
    "api:extract": "api-extractor run --local --verbose",
    "api:document": "api-documenter markdown --input-folder temp --output-folder docs",
    "api:git": "git add --renormalize types docs",
    "docs": "npm-run-all build:types api:extract api:document api:git",
    "clean": "rimraf types types temp dist coverage",
    "prepare": "husky install"
  },
  "dependencies": {
    "ms": "^2.1.3"
  },
  "devDependencies": {
    "@commitlint/cli": "^19.6.1",
    "@commitlint/config-conventional": "^19.6.0",
    "@eslint/js": "^9.17.0",
    "@microsoft/api-documenter": "^7.26.3",
    "@microsoft/api-extractor": "^7.48.1",
    "@rollup/plugin-commonjs": "^28.0.2",
    "@rollup/plugin-node-resolve": "^16.0.0",
    "@rollup/plugin-replace": "^6.0.2",
    "@rollup/plugin-terser": "^0.4.4",
    "@rollup/plugin-typescript": "^12.1.2",
    "@semantic-release/changelog": "^6.0.3",
    "@semantic-release/git": "^10.0.1",
    "@semantic-release/github": "^11.0.1",
    "@semantic-release/npm": "^12.0.1",
    "@trivago/prettier-plugin-sort-imports": "^5.2.0",
    "@types/jest": "^29.5.14",
    "@types/ms": "^0.7.34",
    "@types/node": "^22.10.2",
    "eslint": "^9.17.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.2.1",
    "globals": "^15.14.0",
    "husky": "^9.1.7",
    "jest": "^29.7.0",
    "lint-staged": "^15.2.11",
    "npm-run-all": "^4.1.5",
    "prettier": "^3.4.2",
    "rimraf": "^6.0.1",
    "rollup": "^4.29.1",
    "semantic-release": "^24.2.0",
    "ts-jest": "^29.2.5",
    "tslib": "^2.8.1",
    "typescript": "^5.7.2",
    "typescript-eslint": "^8.18.2"
  },
  "lint-staged": {
    "*.{js,mjs,ts}": [
      "eslint --fix",
      "prettier --write"
    ]
  },
  "resolutions": {
    "hosted-git-info": "^3.0.8",
    "ansi-regex": "^5.0.1",
    "json-schema": "^0.4.0"
  },
  "engines": {
    "node": ">= 18.0.0",
    "npm": ">= 8.6.0",
    "yarn": ">= 1.20.x"
  },
  "buildOptions": {
    "name": "TypeScriptStarterLibrary"
  },
  "publishConfig": {
    "registry": "https://npm.pkg.github.com"
  }
}
